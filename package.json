{
  "name": "playwright-gherkin-reporter",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "version": "0.1.0",
  "author": "Aaron Buxbaum (me@aaronbuxbaum.com)",
  "description": "Playwright reporter for BDD",
  "scripts": {
    "build": "tsup src --format esm --clean",
    "typecheck": "tsc",
    "prepack": "yarn build",
    "format": "prettier --cache --write .",
    "check-formatting": "prettier --cache --check .",
    "publish": "yarn npm publish"
  },
  "dependencies": {
    "@playwright/test": "^1.31.2",
    "cucumber-messages": "^8.0.0",
    "gherkin": "^9.0.0"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19"
  }
}
